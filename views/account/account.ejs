<h1>
    <%= title %>
</h1>

<%- messages() %>
    <% if (errors) { %>
        <ul class="notice">
            <% errors.array().forEach(error=> { %>
                <li>
                    <%= error.msg %>
                </li>
                <% }) %>
        </ul>
        <% } %>

            <h3>Welcome <%= locals.accountData.account_firstname %>
            </h3>
            <p>You're logged in.</p>
            <a title="Click to Edit account information" href="/account/update">Edit Account Information</a>

            <%- checkAccountType %>